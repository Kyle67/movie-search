{
    "env": {
        "browser": true,
        "es2021": true
    },
    // "extends": "standard-with-typescript",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project": "./express/tsconfig.json"
    },
    "rules": {
    },
    "plugins": ["@typescript-eslint"],
    "overrides": [
        {
            "files": ["*.ts", "*.tsx"],
            "rules": {
                "quotes": ["error", "double"],
                "prefer-arrow-callback": "error"
            } 
        }
    ]
}
